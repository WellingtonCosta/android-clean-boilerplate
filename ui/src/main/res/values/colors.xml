<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
        App default palette
        Colors for devices pre marshmallow.

        We are using grey500 for primaryDark because in Android pre marshmallow
        lightStatusBar isn't available.
    -->
    <color name="colorPrimary">@color/white</color>
    <color name="colorPrimaryDark">@color/grey500</color>
    <color name="colorAccent">@color/appMainColor</color>
</resources>
